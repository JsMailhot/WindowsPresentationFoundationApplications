<UserControl x:Class="JourneyApp1.Views.Default"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:JourneyApp1.Views"
             mc:Ignorable="d" Background="Black">
	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="3*"/>
			<ColumnDefinition Width="5*"/>
			<ColumnDefinition Width="1*"/>
			<ColumnDefinition Width="5*"/>
			<ColumnDefinition Width="3*"/>
		</Grid.ColumnDefinitions>
		<Grid.RowDefinitions>
			<RowDefinition Height="3*"/>
			<RowDefinition Height="5*"/>
			<RowDefinition Height="2*"/>
			<RowDefinition Height="5*"/>
			<RowDefinition Height="3*"/>
		</Grid.RowDefinitions>
		<Button FontSize="24" Click="ChangeView" Content="Profile" Grid.Column="1" Grid.Row="1"/>
		<Button FontSize="24" Click="ChangeView" Content="Journals" Grid.Column="3" Grid.Row="1"/>
		<Button FontSize="24" Click="ChangeView" Content="Settings" Grid.Column="1" Grid.Row="3"/>
		<Button FontSize="24" Click="ChangeView" Content="About" Grid.Column="3" Grid.Row="3"/>
	</Grid>
</UserControl>
